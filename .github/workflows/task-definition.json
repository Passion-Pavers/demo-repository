{
    "family": "demo-repository",
    "containerDefinitions": [
        {
            "name": "app",
            "image": "570338923786.dkr.ecr.us-east-1.amazonaws.com/demo-repository",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "80",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::570338923786:role/ecs-task-role",
    "executionRoleArn": "arn:aws:iam::570338923786:role/ecs-task-role",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
